@import "reset";
body{
	min-width:1100px;
}
$imgh:100px;
.pub_tith{
	font-size:30px;
	color:#fff;
	line-height:$imgh;
	padding-left:80px;
}
.pub_tith2{
	font-size:18px;
}
.pub_btn,
.pub_btn2,
.pub_btn3,
.pub_btn4{
	height:30px;
	padding:0 10px;
	color:#fff;
	cursor:pointer;
}
.pub_btn2{
	background:$blue;
}
.pub_btn{
	background:$yellow-dark;
}
.p_wp{
	padding:20px 28px 28px 28px;
}
.pub_btn3{
	background:$gray-darkest;
}
.pub_btn4{
	background:$green;
}
header{
	background:$blue;
	position:relative;
	z-index:5;
	.dropdown-toggle{
		padding:25px 20px 0 10px;
	}
	.font-bold{
		color:$white;
	}
	.text-muted{
		color:$gray-dark;
	}
	.navbar-minimalize{
		position:absolute;
		background:$blue-dark;
		border-color:$blue-dark;
		height:$imgh;
		top:0;
		padding:0;
		margin:0;
		line-height:$imgh;
		width:60px;
		font-size:30px;
	}
}
.img-circle-wp{
	width:$imgh;
	height:$imgh;
	line-height:$imgh;
	overflow:hidden;
}
.img-circle{
	width:$imgh;
}

#wrapper{
	padding-top:$imgh;
	top:-$imgh;
	position:relative;
}
.content-tabs{
	border:1px solid $gray7;
	height:43px;
}
.page-tabs a.active{
	background:#fff;
	border-bottom:2px solid $blue;
}
nav.page-tabs,
.nav.page-tabs{
	height:42px;
}
.mp_wp{
	padding:20px 28px 28px 28px;
	.l,.r{
		width:49%;
	}
	a{
		color:$gray1;
		font-weight:normal;
		font-size:12px;
	}
	.tith{
		font-size:14px;
		margin-bottom:20px;
	}
	#circle_wp,
	#column{
		height:280px;
		width:100%;
		background:$white !important;
	}
	.date_wp{
		text-align:center;
		padding:10px 0;
		background:$white;
	}
}
.dl_file{
	background:url('../img/icon_download.png') no-repeat;
	color:$gray1;
	font-weight:normal;
	font-size:12px;
	padding-left:20px;
}
.sh_wp{
	input{
		font-size:12px;
	}
}
.m_search_wp{
	background:$white;
	padding:10px;
	table{
		width:100%;
		td,th{
			padding-bottom:10px;
		}
		th{
			text-align:right;
		}
	}
	$hsize:24px;
	input[type='text'],
	select{
		width:160px;
		height:$hsize;
		border:1px solid $gray3;
	}
	.pub_btn3{
		width:100px;
	}
}
.m_tabing_wp{
	padding:20px;
	background:$white;
}
.tab{
	li{
		float:left;
		padding:10px;
		border-bottom:3px solid transparent;
		margin-bottom:-2px;
		cursor:pointer;
		&.ac{
			border-color:$yellow-dark;
			color:$yellow-dark;
		}
	}
	border-bottom:1px solid $gray4;
}
.msg_wp{
	padding:20px;
	li{
		margin-bottom:10px;
	}
	em{
		color:$blue;
	}
	b{
		color:$red;
	}
}
.tab_wp2{
	background:$white;
	padding:20px;
	.tg{
		width:80%;
		margin:0 auto;
		li{
			float:left;
			width:20%;
			text-align:center;
			cursor:pointer;
			position:relative;
		}
		$s:40px;
		em{
			display:block;
			font-weight:bold;
			font-style:normal;
			color:$white;
			background:$gray6;	
			width:$s;
			height:$s;
			margin:0 auto 10px auto;
			border-radius:$s;
			line-height:$s;
			position:relative;
			z-index:2;
		}
		b{
			position:absolute;
			height:0px;
			width:100%;
			border-bottom:2px solid $gray6;
			right:-50%;
			top:$s/2;
			z-index:1;
			overflow:hidden;
		}
		.finish{
			em{
				background:$blue;
			}
			strong{
				color:$blue;
			}
			b{
				border-bottom:2px solid $blue;
			}
		}
		.working{
			em{
				background:$green;
			}
			b{
				border-bottom:2px dashed $green;
			}
		}
	}
	color:$gray6;
}
.forms{
	display:none;
}
.swreg_wp{
	th{
		text-align:right;
		vertical-align:top;
	}
	td{
		padding-bottom:10px;
	}
	$hsize:24px;
	textarea,
	input[type='text'],
	select{
		width:160px;
		height:$hsize;
		border:1px solid $gray4;
		resize:none;
	}
	input[type='text']{
		width:155px;
	}	
	textarea{
		height:180px;
		width:400px; 
	}
}
.sw_tith{
	font-size:18px;
	margin-bottom:10px;
}
.wring{
	color:#f00;
	font-size:12px;
	font-weight:normal;
}
.kharea{
	width:100%;
	height:100px;
	border:1px solid $gray4;
	resize:none;
	display:block
}
.swreg_wp{
	em{
		color:#f00;
	}
}
.export{
	background:url('../img/icon_export.png') no-repeat $blue 10px 50%;
	padding:10px 10px 10px 30px;
	font-size:12px;
	color:$white;
	display:block;
	font-weight:normal;
	border-radius:5px;
}
.ptb_wp{
	table{
		width:100%;
		background:$white;
	}
	.i0{
		width:40%;
	}
	.i1{
		width:58%;
		text-align:center;
	}
	.k{
		td,
		th{
			text-align:left;
			padding-left:5px;
		}
	}
	.tith{
		color:$gray1;
		font-weight:normal;
		margin-bottom:10px;
		font-size:14px;
		text-align:left;
	}
	.fst{
		th{
			background:$yellow-dark;
			color:$white;
		}
	}
	td,
	th{
		font-weight:normal;
		padding:10px 0;
		border:1px solid $gray;
	}
}
.blist{
	li{
		width:48%;
		#circle_wp,#column{
			height:260px;
		}
	}
}
