.wstatistic_left table thead th {
    border: 1px solid #CECFD3;
    height: 50px;
}

.wstatistic_left table tbody tr td {
    height: 50px;
    border: 1px solid #ECECEE;
    text-align: center;
    background-color: #fff;
}

.wstatistic_left th {
    width: 102px;
    /*鏉╂瑩鍣烽棁锟界憰浣藉殰瀹歌精鐨熼弫杈剧礉閺嶈宓侀懛顏勭箒閻ㄥ嫰娓跺Ч鍌濈殶閺佹潙顔旀惔锟�*/
    height: 50px;
    /*鏉╂瑩鍣烽棁锟界憰浣藉殰瀹歌精鐨熼弫杈剧礉閺嶈宓侀懛顏勭箒閻ㄥ嫰娓跺Ч鍌濈殶閺佹挳鐝惔锟�*/
    position: relative;
    background-color: #E3E4E6;
    border: 1px solid #CECFD2;
    color: #525768;
    text-align: center;
}

.wstatistic_left th[class=first]:before {
    content: "";
    position: absolute;
    width: 1px;
    height: 130px;
    top: 0;
    left: 0;
    background-color: #CECFD3;
    display: block;
    transform: rotate(-68deg);
    transform-origin: top;
}

.wstatistic_left th[class=first] span.count {
    position: relative;
    left: -24px;
    top: 9px;
}

.wstatistic_left th[class=first] span.staitem {
    position: relative;
    left: 20px;
    top: -4px;
}

