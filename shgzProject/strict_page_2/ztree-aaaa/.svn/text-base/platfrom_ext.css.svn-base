html,body{
min-width:940px; 
}
.ui_menu {
padding:2px 4px;
}
.ui_menu ul{ list-style-type:none;}  
.ui_menu a{ 
display:block;
text-decoration:none;
cursor: pointer; 
color:#0033CC;
font-size:13px;
padding-left:10px;
height:24px;
line-height:24px;
}  
.ui_menu a:hover{ 
	background: #eaf2ff;
	border-left:solid 6px #cecece;
}
.hover_cls{
	background: #eaf2ff;
	border-left:solid 6px #cecece;
}
table{
	border-collapse:collapse;
}
.form_table{ 
    display:table; 
	table-layout:auto; 
	height: auto; 
	margin: 0px auto; 
	font-size:12px;
 }
 .form_table td{
 	padding:5px 3px;
  	border-bottom:1px solid #ddd;
  	text-align:left;
  	height:26px;
  	line-height:26px;
 }
 .form_table .tdhead{
 	color: #444; 
 	text-align: left; 
 	background: #F5F5F5; 
 	font-size: 16px;
 }
.form_table  .col_right{
    text-align:right;
	color:#999;
	font-size:13px;
	padding:1px 2px;
 }
.form_table  .col_center{
    text-align:center;
	color:#6093ac;
	background:#f6f6f6;
	font-size:13px;
	padding:1px 2px;
 }
input[type="text"],input[type="password"],select{
	padding-left: 3px;
	font-family:sans-serif;
	height: 26px;
	padding: 2px 4px;
	font-size: 14px;
	line-height: 1.42857143;
	color: #555;
	background-color: #fff;
	border: 1px solid #ccc;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	-webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
	-o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
	transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
}
textarea{
	line-height: 18px;
	-webkit-appearance: none;
	vertical-align: middle;
	font-family:sans-serif;
	display: block;
	width: 98%;
	height: 57px;
	padding: 6px 8px;
	font-size: 14px;
	line-height: 1.42857143;
	color: #555;
	background-color: #fff;
	border: 1px solid #ccc;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	box-shadow: inset 0 1px 1px rgba(0,0,0,.075);
	-webkit-transition: border-color ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;
	-o-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
	transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
}
input[readOnly],textarea[readonly]{
	margin-left: 0;
	overflow: hidden!important;
	white-space: nowrap!important;
	color: #777!important;
	background-color: #fafafa!important;
	border-color: #ccc!important;
	-webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.025)!important;
	-moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.025)!important;
	box-shadow: inset 0 1px 2px rgba(0,0,0,0.025)!important;
}
input[type="range"] {
  display: block;
  width: 100%;
}
select[multiple],
select[size] {
  height: auto;
}
input[type="file"]:focus,
input[type="radio"]:focus,
input[type="checkbox"]:focus {
  outline: thin dotted;
  outline: 5px auto -webkit-focus-ring-color;
  outline-offset: -2px;
}
