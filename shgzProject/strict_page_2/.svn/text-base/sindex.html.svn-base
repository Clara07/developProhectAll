	<script src="js/jquery-1.11.3.min.js"></script>
	<script type="text/javascript">
		$(function(){
			var tabbing = $('#tabbing li'),
				className = 'ac',
				cts = $('#cts table'),
				hd = 'hd';

			tabbing.mouseenter(function(){
				var idx = $(this).index();
				if(idx != 0){
					tabbing.removeClass(className);
					$(this).addClass(className);

					cts.addClass(hd);
					cts.eq(idx-1).removeClass(hd);
				}
			});
		});
	</script>
